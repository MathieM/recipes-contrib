{
    "manifests": {
        "enhavo/media-bundle": {
            "manifest": {
                "bundles": {
                    "Enhavo\\Bundle\\MediaBundle\\EnhavoMediaBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "assets/": "assets/"
                },
                "post-install-output": [
                    "<bg=blue;fg=white>                                               </>",
                    "<bg=blue;fg=white>  Complete EnhavoMediaBundle installation ...  </>",
                    "<bg=blue;fg=white>                                               </>",
                    "",
